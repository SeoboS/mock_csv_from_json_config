{
    "file_name": "mock_financial_data.csv",
    "rows": 1000000,
    "columns": {
        "asOfDate": {
            "type": "enum",
            "values": ["20190630","20190731","20190830","20190930","20191031","20191130"]
        },
	"fico": {
